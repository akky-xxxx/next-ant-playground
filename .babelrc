{
  "presets": [
    "next/babel"
  ],
  "plugins": [
    "@babel/plugin-proposal-optional-chaining",
    "@babel/plugin-proposal-nullish-coalescing-operator",
    [
      "styled-components",
      {
        "ssr": true
      }
    ],
    [
      "import",
      {
        "libraryName": "antd",
        "style": "css"
      },
      "ant"
    ],
    [
      "import",
      {
        "libraryName": "antd-mobile",
        "style": "css"
      },
      "antd-mobile"
    ]
  ]
}
