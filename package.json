{
  "name": "next-ant-playground",
  "version": "0.0.0",
  "author": "Shogo Akimoto <114.s.aki@gmail.com>",
  "license": "MIT",
  "repository": "https://github.com/akky-xxxx/next-ant-playground.git",
  "scripts": {
    "agreed-ui": "agreed-ui --path ./src/api/agreed.ts --port 3031",
    "build": "run-p build:*",
    "build:client": "next build ./src/client",
    "build:server": "parcel build src/server/start-server.ts -d bin --target node",
    "check-all": "npm-run-all fix -p lint type-check spell-check",
    "dev": "run-s build serve",
    "docker": "run-s docker:build docker:run",
    "docker:build": "docker build ./ -t next-ant-playground/fe",
    "docker:run": "docker run -p 30000:3000 -d next-ant-playground/fe",
    "fix": "run-p fix:*",
    "fix:api": "eslint -c ./src/api/.eslintrc.js --ext .ts ./src/api --fix",
    "fix:client": "eslint -c src/client/.eslintrc.js --ext .ts,.tsx ./src/client --fix",
    "fix:json": "run-p fix:json:*",
    "fix:json:1": "prettier ./*.json --write",
    "fix:json:2": "prettier ./src/**/*.json --write",
    "fix:pack": "fixpack",
    "fix:root": "eslint ./*.js --fix",
    "fix:server": "eslint -c ./src/server/.eslintrc.js --ext .ts, ./src/server --fix",
    "lint": "run-p lint:*",
    "lint:api": "eslint -c ./src/api/.eslintrc.js --ext .ts ./src/api",
    "lint:client": "eslint -c src/client/.eslintrc.js --ext .ts,.tsx ./src/client",
    "lint:css": "stylelint './src/**/*.{js,jsx,ts,tsx}'",
    "lint:root": "eslint ./*.js",
    "lint:server": "eslint -c ./src/server/.eslintrc.js --ext .ts, ./src/server",
    "serve": "run-p serve:*",
    "serve:api": "agreed-server --path ./src/api/agreed.ts --port 3010",
    "serve:bff": "cross-env DEBUG=app:server:* node bin/start-server.js",
    "spell-check": "cspell './src/**/*.{ts,tsx}'",
    "type-check": "run-p type-check:*",
    "type-check:api": "tsc -p ./src/api --noEmit",
    "type-check:client": "tsc -p ./src/client --noEmit",
    "type-check:server": "tsc -p ./src/server --noEmit"
  },
  "dependencies": {
    "@zeit/next-css": "1.0.1",
    "@zeit/next-less": "1.0.1",
    "antd": "3.26.11",
    "antd-mobile": "2.3.1",
    "antd-pro": "0.7.0",
    "axios": "0.19.2",
    "axios-middleware": "0.3.1",
    "cross-env": "7.0.0",
    "date-fns": "2.9.0",
    "debug": "4.1.1",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-session": "1.17.0",
    "favalid": "0.9.1",
    "fetchr": "0.5.38",
    "gtoken": "4.1.4",
    "less": "3.11.1",
    "memorystore": "1.6.2",
    "next": "9.2.2",
    "next-routes": "1.4.2",
    "npm-run-all": "4.1.5",
    "passport": "0.4.1",
    "passport-google-oauth20": "2.0.0",
    "rc-queue-anim": "1.8.3",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-redux": "7.2.0",
    "redux": "4.0.5",
    "redux-actions": "2.6.5",
    "redux-effects-fetchr": "1.1.0",
    "redux-effects-steps": "1.0.3",
    "remeda": "0.0.15",
    "reselect": "4.0.0",
    "styled-components": "5.0.1",
    "uuid": "3.4.0"
  },
  "devDependencies": {
    "@babel/core": "7.8.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.8.3",
    "@babel/plugin-proposal-optional-chaining": "7.8.3",
    "@storybook/react": "5.3.13",
    "@testing-library/jest-dom": "5.1.1",
    "@testing-library/react": "9.4.0",
    "@types/axios": "0.14.0",
    "@types/date-fns": "2.6.0",
    "@types/debug": "4.1.5",
    "@types/dotenv": "8.2.0",
    "@types/express": "4.17.2",
    "@types/express-session": "1.15.16",
    "@types/jest": "25.1.3",
    "@types/passport": "1.0.2",
    "@types/passport-google-oauth20": "2.0.3",
    "@types/react": "16.9.22",
    "@types/react-dom": "16.9.5",
    "@types/react-redux": "7.1.7",
    "@types/redux-actions": "2.6.1",
    "@types/styled-components": "5.0.0",
    "@types/uuid": "3.4.7",
    "@typescript-eslint/eslint-plugin": "2.20.0",
    "@typescript-eslint/parser": "2.20.0",
    "agreed": "3.2.2",
    "agreed-typed": "1.0.1",
    "babel-loader": "8.0.6",
    "babel-plugin-import": "1.13.0",
    "cspell": "4.0.50",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.18.3",
    "fixpack": "3.0.5",
    "husky": "4.2.3",
    "jest": "25.1.0",
    "lint-staged": "10.0.7",
    "next-compose-plugins": "2.2.0",
    "null-loader": "3.0.0",
    "parcel-bundler": "1.12.4",
    "prettier": "1.19.1",
    "react-docgen-typescript-webpack-plugin": "1.1.0",
    "stylelint": "13.2.0",
    "stylelint-config-standard": "20.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-order": "4.0.0",
    "stylelint-processor-styled-components": "1.10.0",
    "ts-jest": "25.2.1",
    "ts-node": "8.6.2",
    "typescript": "3.8.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn check-all"
    }
  },
  "lint-staged": {
    "*.ts*": [
      "yarn fix:client",
      "yarn lint:client",
      "yarn lint:css",
      "git add"
    ]
  },
  "private": true
}
