{
  "name": "next-ant-playground",
  "version": "0.0.0",
  "author": "Shogo Akimoto <114.s.aki@gmail.com>",
  "license": "MIT",
  "repository": "https://github.com/akky-xxxx/next-ant-playground.git",
  "scripts": {
    "agreed-ui": "agreed-ui --path ./src/api/agreed.ts --port 3031",
    "build": "run-p build:*",
    "build:client": "next build ./src/client",
    "build:server": "parcel build src/server/start-server.ts -d bin --target node",
    "check-all": "npm-run-all fix -p lint type-check spell-check",
    "dev": "npm-run-all build serve",
    "fix": "run-p fix:*",
    "fix:api": "eslint -c ./src/api/.eslintrc.js --ext .ts ./src/api --fix",
    "fix:client": "eslint -c src/client/.eslintrc.js --ext .ts,.tsx ./src/client --fix",
    "fix:json": "run-p fix:json:*",
    "fix:json:1": "prettier ./*.json --write",
    "fix:json:2": "prettier ./src/**/*.json --write",
    "fix:pack": "fixpack",
    "fix:root": "eslint ./*.js --fix",
    "fix:server": "eslint -c ./src/server/.eslintrc.js --ext .ts, ./src/server --fix",
    "lint": "run-p lint:*",
    "lint:api": "eslint -c ./src/api/.eslintrc.js --ext .ts ./src/api",
    "lint:client": "eslint -c src/client/.eslintrc.js --ext .ts,.tsx ./src/client",
    "lint:css": "stylelint './src/**/*.{js,jsx,ts,tsx}'",
    "lint:root": "eslint ./*.js",
    "lint:server": "eslint -c ./src/server/.eslintrc.js --ext .ts, ./src/server",
    "serve": "run-p serve:*",
    "serve:api": "agreed-server --path ./src/api/agreed.ts --port 3010",
    "serve:customServer": "cross-env DEBUG=app:server:* node bin/start-server.js",
    "spell-check": "cspell './src/**/*.{ts,tsx}'",
    "type-check": "run-p type-check:*",
    "type-check:api": "tsc -p ./src/api --noEmit",
    "type-check:client": "tsc -p ./src/client --noEmit",
    "type-check:server": "tsc -p ./src/server --noEmit"
  },
  "dependencies": {
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-less": "^1.0.1",
    "agreed-typed": "^1.0.1",
    "antd": "^3.25.0",
    "antd-mobile": "^2.3.1",
    "antd-pro": "^0.6.15",
    "axios": "^0.19.0",
    "cross-env": "^6.0.3",
    "date-fns": "^2.7.0",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "favalid": "^0.9.1",
    "fetchr": "^0.5.38",
    "less": "^3.10.3",
    "next": "^9.1.3",
    "next-compose-plugins": "^2.2.0",
    "next-routes": "^1.4.2",
    "rc-queue-anim": "^1.8.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-redux": "^7.1.3",
    "redux": "^4.0.4",
    "redux-actions": "^2.6.5",
    "redux-effects-fetchr": "^1.1.0",
    "redux-effects-steps": "^1.0.3",
    "remeda": "^0.0.12",
    "reselect": "^4.0.0",
    "styled-components": "^4.4.1",
    "uuid": "^3.3.3",
    "validator": "^12.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@storybook/react": "^5.2.5",
    "@types/axios": "^0.14.0",
    "@types/date-fns": "^2.6.0",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.2",
    "@types/jest": "^24.0.22",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.5",
    "@types/redux-actions": "^2.6.1",
    "@types/styled-components": "^4.1.22",
    "@types/uuid": "^3.4.6",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "agreed": "^3.2.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.12.2",
    "cspell": "^4.0.33",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "fixpack": "^2.3.1",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "jest-dom": "^4.0.0",
    "lint-staged": "^9.4.2",
    "npm-run-all": "^4.1.5",
    "null-loader": "^3.0.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "^1.18.2",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "react-testing-library": "^8.0.1",
    "stylelint": "^11.1.1",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^3.1.1",
    "stylelint-processor-styled-components": "^1.8.0",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.7.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn check-all"
    }
  },
  "lint-staged": {
    "*.ts*": [
      "yarn fix:client",
      "yarn lint:client",
      "yarn lint:css",
      "git add"
    ]
  },
  "private": true
}
